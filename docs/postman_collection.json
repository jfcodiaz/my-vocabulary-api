				{
					"name": "login as default user",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Primero, solicita el cookie CSRF para iniciar sesi√≥n.",
									"pm.sendRequest({",
									"    url: 'http://localhost:8080/sanctum/csrf-cookie',",
									"    method: 'GET'",
									"}, function (error, response, {cookies}) {",
									"    if (!error) {",
									"        pm.collectionVariables.set('xsrf-cookie', cookies.get('XSRF-TOKEN'));",
									"",
									"        // Una vez que tienes el cookie CSRF, inicia sesi√≥n.",
									"        pm.sendRequest({",
									"            url: 'http://localhost:8080/login',",
									"            method: 'POST',",
									"            header: {",
									"                'Content-Type': 'application/json',",
									"                'X-XSRF-TOKEN': pm.collectionVariables.get('xsrf-cookie')",
									"            },",
									"            body: {",
									"                mode: 'raw',",
									"                raw: JSON.stringify({",
									"                    \"email\": \"user@serv.com\",",
									"                    \"password\": \"password\"",
									"                })",
									"            }",
									"        }, function (error, response) {",
									"            if (!error) {",
									"                var responseData = response.json();",
									"                console.log(responseData);",
									"                // Guarda el token recibido en una variable.",
									"                pm.collectionVariables.set(\"token\", responseData.token);",
									"                console.log(responseData.token)",
									"                console.log(\"Token set in collection variables:\", responseData.token);",
									"            }",
									"        });",
									"    }",
									"});",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
